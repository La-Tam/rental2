@{
    ViewData["Title"] = "Index";
}

<head>
    <meta charset="UTF-8">
    <style>
        .app-menu__item.property, app-menu__item:hover, app-menu__item:focus {
            background: #0d1214;
            text-decoration: none;
            color: #fff;
        }
    </style>
    <link href="~/wwwroot/css/main.css" rel="stylesheet" />
</head>
<main class="app-content" style="margin-top:20px">
    <div class="row">
        <div class="col-lg-3">
            <p class="bs-component">
                <div class="form-group">
                    <select class="form-control" id="exampleSelect1">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                    </select>
                </div>
            </p>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="tile mb-4" style="min-height: calc(90vh - 50px);">
                <div class="page-header">
                    <div class="tile-title-w-btn">
                        <h2 class="title" id="navs" style="color:#009688">property test</h2>
                        <div class="btn-group">
                            <a class="btn btn-primary" href="@Url.Action("Update", "Property")"><i class="fa fa-lg fa-edit"></i></a>
                            <a class="btn btn-primary" onclick="return confirm();" href="#"><i class="fa fa-lg fa-trash"></i></a>
                            <script src="~/wwwroot/js/plugins/pace.min.js"></script>
                            <script type="text/javascript" src="~/wwwroot/js/plugins/sweetalert.min.js"></script>
                            <script type="text/javascript">
                                function confirm() {
                                    swal({
                                        title: '@Rental_WebCore2.Resources.Property.Property_Index.Do_you_want_to_delete_this_data',
                                        text: '@Rental_WebCore2.Resources.Property.Property_Index.You_will_not_be_able_to_restore_this_data',
                                        type: "warning",
                                        showCancelButton: true,
                                        confirmButtonClass: "btn-danger",
                                        confirmButtonText: "@Rental_WebCore2.Resources.Property.Property_Index.Yes",
                                        cancelButtonText: "@Rental_WebCore2.Resources.Property.Property_Index.No",
                                        closeOnConfirm: false,
                                        closeOnCancel: false
                                    }, function (isConfirm) {
                                        if (isConfirm) {
                                            swal({
                                                title: "@Rental_WebCore2.Resources.Property.Property_Index.Deleted",
                                                type: "success",
                                                text: "@Rental_WebCore2.Resources.Property.Property_Index.Your_data_has_been_deleted",
                                                confirmButtonText: "@Rental_WebCore2.Resources.Property.Property_Index.Complete",
                                                showConfirmButton: true
                                            }, function () {
                                                window.location.href = '/@ViewContext.RouteData.Values["language"].ToString()/Property/Delete';
                                            });
                                        } else {
                                            swal({
                                                title: "@Rental_WebCore2.Resources.Property.Property_Index.Canceled",
                                                type: "error",
                                                text: "@Rental_WebCore2.Resources.Property.Property_Index.The_data_has_been_retained",
                                                confirmButtonText: "@Rental_WebCore2.Resources.Property.Property_Index.OK",
                                                showConfirmButton: true
                                            });
                                        }
                                    }
                                    )
                                }
                            </script>
                        </div>
                    </div>
                    <div class="row" style="margin-bottom:2rem;">
                        <div class="col-lg-12">
                            <div class="bs-component">
                                <div class="row">
                                    <div class="col-lg-8">
                                        <ul class="nav nav-tabs">
                                            @if (true)
                                            {
                                                <li class="nav-item"><a class="nav-link active show" data-toggle="tab" href="#property"><i class="icon fa fa-building-o"></i> @Rental_WebCore2.Resources.Property.Property_Index.Property_Enclosed</a></li>
                                            }
                                            else
                                            {
                                                <li class="nav-item"><a class="nav-link disabled" data-toggle="tab" href="#property"><i class="icon fa fa-building-o"></i> Bất động sản kèm theo</a></li>
                                            }
                                            <li class="nav-item"><a class="nav-link" data-toggle="tab" href="#detail"><i class="icon fa fa-info"></i> @Rental_WebCore2.Resources.Property.Property_Index.Detail</a></li>
                                            <li class="nav-item"><a class="nav-link" data-toggle="tab" href="#asset"><i class="icon fa fa-diamond"></i> @Rental_WebCore2.Resources.Property.Property_Index.Assets_Attached</a></li>
                                        </ul>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="page-header">
                                            <div class="tile-title-w-btn">
                                                <h2 class="title" id="navs"></h2>
                                                <div class="btn-group">
                                                    @if (true)
                                                    {
                                                        <a class="btn btn-primary" href="@Url.Action("AddNew", "Property")"><i class="fa fa-lg fa-plus"></i> @Rental_WebCore2.Resources.Property.Property_Index.Add_Property_Enclosed</a>
                                                    }
                                                    else
                                                    {
                                                        <button class="btn btn-primary" disabled><i class="fa fa-lg fa-plus"></i> Thêm bất động sản kèm theo</button>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-content" id="myTabContent">
                                    @if (true)
                                    {
                                        <div class="tab-pane fade" id="detail">
                                            <div class="tile">
                                                <div class="tile-body">
                                                    <form>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">@Rental_WebCore2.Resources.Property.Property_Detail.Name_of_property</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="test">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">@Rental_WebCore2.Resources.Property.Property_Detail.Address</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="384 Hoàng Diệu">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">@Rental_WebCore2.Resources.Property.Property_Detail.Province_City</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="TP.Hồ Chí Minh">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">@Rental_WebCore2.Resources.Property.Property_Detail.District</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="Q.4">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">@Rental_WebCore2.Resources.Property.Property_Detail.Wards</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="P.6">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">@Rental_WebCore2.Resources.Property.Property_Detail.Type_of_property</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="Tòa nhà">
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="tab-pane fade active show" id="detail">
                                            <div class="tile">
                                                <div class="tile-body">
                                                    <form>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">Tên bất động sản</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="test">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">Địa chỉ</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="384 Hoàng Diệu">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">Tỉnh/Thành phố</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="TP.Hồ Chí Minh">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">Quận/Huyện</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="Q.4">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">Phường/Xã</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="P.6">
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label" style="font-size:20px">Loại hình bất động sản</label>
                                                            <input class="form-control" id="disabledInput" type="text" disabled="" value="Tòa nhà">
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    @if (true)
                                    {
                                        <div class="tab-pane fade active show" id="property">
                                            @for (var k = 0; k < 4; k++)
                                            {
                                                <div class="row">
                                                    @for (var j = 0; j < 3; j++)
                                                    {
                                                        <div class="col-lg-4">
                                                            @{
                                                                <a class="list-group-item list-group-item-action" href="/@ViewContext.RouteData.Values["language"].ToString()/@ViewContext.RouteData.Values["controller"].ToString()" style="border-color:#009688">
                                                                    <h4 class="list-group-item-heading">test</h4>
                                                                    <p class="list-group-item-text">Donec id elit non mi porta gravida at eget metus. Maecenas sed diam eget risus varius blandit.</p>
                                                                </a>
                                                            }
                                                        </div>
                                                    }
                                                </div>
                                                <br />
                                            }
                                        </div>
                                    }
                                    <div class="tab-pane fade" id="asset">
                                        <table class="table table-bordered">
                                            <tr style="color:#009688; text-align:center; ">
                                                <td><strong>STT</strong></td>
                                                <td><strong>Tên</strong></td>
                                                <td><strong>Mã Tài Sản</strong></td>
                                                <td><strong>Tình Trạng</strong></td>
                                                <td><strong>Chủ Sở Hữu</strong></td>
                                                <td><strong>Báo Hỏng</strong></td>
                                                <td><strong>Thao Tác</strong></td>
                                            </tr>
                                            <tr>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                            </tr>
                                        </table>
                                        <div class="tile-footer" align="left">
                                            <a class="btn btn-primary" href="@Url.Action("AddNew", "Property")">@Rental_WebCore2.Resources.Property.Property_Index.Assign</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>