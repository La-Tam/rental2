                </div>
                <hr style="margin-top:-10px" />
                <div class="row">

                    <a class="btn btn-primary" style="background-color: #007065;color:white;margin-left:15px">@Rental_WebCore2.Resources.Rental_Manager.Rental.Save</a>
                    <a class="btn btn-primary" style="background-color: #007065;color:white;margin-left:20px;" href="@Url.Action("ContractIndex","RentalManager")">@Rental_WebCore2.Resources.Rental_Manager.Rental.Cancel</a>

                </div>

            </div>

        </div>
    </main>
</body>

@* Add dich vu *@

<div class="modal fade" id="myModalService">
    <div class="modal-dialog">
        <div class="modal-content">
            <text class="form-control" style="background-color:#007065; height:50px;color:aliceblue;border:hidden; text-align:center;font-size:25px; padding-left:0px;border-radius:unset" id="modalTitleService;">
                @Rental_WebCore2.Resources.Rental_Manager.Rental.Add_Service
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="color:white; font-size:30px">
                    <span aria-hidden="true">&times;</span>
                </button>
            </text>
            <div class="modal-header">
            </div>
            <div class="modal-body">
                <form id="form">

                    <div class="form-group">
                        <label for="exampleInputEmail1">@Rental_WebCore2.Resources.Rental_Manager.Rental.Type_Of_Service</label>
                        <input class="form-control" id="exampleInputEmail1" type="text" aria-describedby="emailHelp" placeholder="Nhập loại dịch vụ">
                    </div>

                    <div class="form-group">
                        <label for="exampleInputEmail1">@Rental_WebCore2.Resources.Rental_Manager.Rental.Quantity</label>
                        <input class="form-control" id="exampleInputEmail1" type="text" aria-describedby="emailHelp" placeholder="Nhập số lượng">
                    </div>

                    <div class="form-group">
                        <label for="exampleInputEmail1">@Rental_WebCore2.Resources.Rental_Manager.Rental.Unit_Price</label>
                        <input class="form-control" id="exampleInputEmail1" type="text" aria-describedby="emailHelp" placeholder="Nhập đơn giá">
                    </div>

                    <div class="form-group">
                        <label for="exampleInputEmail1">@Rental_WebCore2.Resources.Rental_Manager.Rental.Unit</label>
                        <input class="form-control" id="exampleInputEmail1" type="text" aria-describedby="emailHelp" placeholder="Nhập đơn vị">
                    </div>


                </form>
                @*  *@
                <div class="modal-footer">
                    <a class="btn btn-primary" style="background-color: #007065;color:white">@Rental_WebCore2.Resources.Rental_Manager.Rental.Save</a>
                    <button class="btn btn-primary" style="background-color: #007065;color:white" data-dismiss="modal">@Rental_WebCore2.Resources.Rental_Manager.Rental.Cancel</button>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    function AddService(addservice) {
        $("#form")[0].reset();
        $("#modalTitleService").html("Thêm dịch vụ");
        $("#myModalService").modal();
    }
</script>

