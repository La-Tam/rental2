@{
    ViewData["Title"] = "DashboardIndex";
}
@{
    string[] name = new string[15];
    int i = 0;
}
<html>
<head>
    <link href="~/wwwroot/CSS_Rental/css/main.css" rel="stylesheet" />
    <style type="text/css">
        a:hover, a:focus {
            text-decoration: none;
        }
    </style>

</head>

<body>
    <main class="app-content">
        <div class="page-content" style="height:100%">

            <!--PAGE CONTENT BEGINS HERE-->
            <style>
                #select_project_wrap {
                    display: none;
                }

                .page-content {
                    max-width: 100%;
                    margin: auto;
                    border-top: none;
                    background: none;
                }

                #select_application_wrap {
                    display: none;
                }

                .widget-box {
                    margin-bottom: 20px;
                }

                .widget-body {
                    min-height: 280px;
                }

                .btn-home-add {
                    width: 120px;
                    height: 120px;
                    display: block;
                    line-height: 118px;
                    text-align: center;
                    border: 2px solid #ccc;
                    border-radius: 50%;
                    margin: 0px auto;
                }
            </style>

            @* Thong ke *@
            <div class="row">
                <div class="col-sm-6">

                    <div class="tile">
                        <div class="widget-header">
                            <h5 class="widget-title">@Rental_WebCore2.Resources.Rental_Manager.Rental.Statistic</h5>
                        </div>
                        <hr />
                        <div class="widget-box">
                            <div class="widget-main">
                                <div id="Dashboard_Summary">
                                    <style>
                                    </style>
                                    <div class="col-md-12">
                                        <div class="row" style="padding-top:20px; padding-bottom:80px">
                                            <div id="thongke_nha" class="col-md-6">
                                                <div>
                                                    @*<a class="btn-home-add" href="/REProduct">+ thêm mới</a>*@
                                                    <img src="~/wwwroot/images/Rental/Dashboard/house.jpg" style="width:100px;margin-top:-20px;margin-left:30px" />
                                                </div>
                                                <div style="margin-top:20px;margin-left:25px">
                                                    <h3>
                                                        <a href="">
                                                            <span style="color:#333"><b>12</b></span>
                                                            <span style="color:none">@Rental_WebCore2.Resources.Rental_Manager.Rental.House</span>
                                                        </a>
                                                    </h3>
                                                    <p>
                                                        <a href="/REProduct?Status=empty">
                                                            <span class="label label-warning" style="width: 10px; height: 10px; display: inline-block; padding: 0px;background-color:red"></span>
                                                            <span style="color:#333">
                                                                <b>
                                                                    7
                                                                </b>
                                                            </span>
                                                            @Rental_WebCore2.Resources.Rental_Manager.Rental.Empty_House
                                                        </a>
                                                    </p>
                                                    <p>
                                                        <a href="#">
                                                            <span class="label label-success" style="width: 10px; height: 10px; display: inline-block; padding: 0px;background-color:blue"></span>
                                                            <span style="color:#333">
                                                                <b>
                                                                    5
                                                                </b>
                                                            </span>
                                                            @Rental_WebCore2.Resources.Rental_Manager.Rental.Hired_House
                                                        </a>
                                                    </p>
                                                </div>
                                            </div>
                                            @* Thong ke phong *@
                                            <div id="thongke_phong" class="col-md-6">
                                                <div>
                                                    @*<a class="btn-home-add" href="/REProduct">+ thêm mới</a>*@
                                                    <img src="~/wwwroot/images/Rental/Dashboard/room.jpg" style="width:100px;margin-top:-20px;margin-left:40px" />
                                                </div>
                                                <div class="pull-right" style="margin-top:30px;margin-right:55px">
                                                    <h3>
                                                        <a href="">
                                                            <span style="color:#333"><b>70</b></span>
                                                            <span style="color:none">@Rental_WebCore2.Resources.Rental_Manager.Rental.Room</span>
                                                        </a>
                                                    </h3>
                                                    <p>
                                                        <a href="/REProduct?CheckAvailableSlot=True">
                                                            <span class="label label-warning" style="width: 10px; height: 10px; display: inline-block; padding: 0px;background-color:red"></span>
                                                            <span style="color:#333">
                                                                <b>
                                                                    40
                                                                </b>
                                                            </span>
                                                            @Rental_WebCore2.Resources.Rental_Manager.Rental.Empty_Room
                                                        </a>
                                                    </p>
                                                    <p>
                                                        <a href="/REProduct?CheckAvailableSlot=False">
                                                            <span class="label label-success" style="width: 10px; height: 10px; display: inline-block; padding: 0px;background-color:blue"></span>
                                                            <span style="color:#333">
                                                                <b>
                                                                    30
                                                                </b>
                                                            </span>
                                                            @Rental_WebCore2.Resources.Rental_Manager.Rental.Hired_Room
                                                        </a>
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    @* Dien nuoc *@
                    <div class="tile" style="height:220px">
                        <div class="widget-header">
                            <h5 class="widget-title">@Rental_WebCore2.Resources.Rental_Manager.Rental.Electricity_Water</h5>
                            <hr />
                        </div>
                        <div class="widget-body">
                            <div class="widget-main">
                                <div id="Dashboard_SummaryInvoiceEW">
                                    <div class="clearfix">
                                        <a href="@Url.Action("ElectricityAndWaterIndex","RentalManager")">
                                            <div class="btn-outline-primary" style="border:solid;border-color:darkgrey;border-width:1px;margin-top:30px">
                                                <div class="infobox-icon">
                                                    <img src="~/wwwroot/images/Rental/Dashboard/icons8-calculator-50.png" style="width:35px;margin-left:7px;margin-top:8px" />
                                                </div>
                                                <div class="infobox-data" style="margin-left:45px;margin-top:-39px">
                                                    <span class="infobox-data-number ng-binding">0</span>
                                                    <div class="infobox-content" style="text-decoration: underline;">@Rental_WebCore2.Resources.Rental_Manager.Rental.Product_Record_Elect_Water<ul></ul></div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                @* Hop dong *@
                <div class="col-sm-6">
                    <div class="tile" style="height:220px">
                        <div class="widget-header">
                            <h5 class="widget-title">@Rental_WebCore2.Resources.Rental_Manager.Rental.Contract</h5>
                            <hr>
                        </div>
                        <div class="widget-body">
                            <div class="widget-main">
                                <div id="Dashboard_Contract">

                                    @* HD sap het han *@
                                    <div class="clearfix">
                                        <div class="col-md-12">
                                            <div class="row">

                                                <div class=" btn-outline-primary col-md-6" style="padding-top:3px;border:solid;border-width:1px;border-color:darkgrey">
                                                    <a href="@Url.Action("ContractAboutExp","RentalManager")">
                                                        <div class="infobox infobox-red">
                                                            <div class="infobox-icon">
                                                                <img src="~/wwwroot/images/Rental/Dashboard/icons8-red-file-64.png" style="width:40px;margin-left:0px;margin-top:5px" />
                                                            </div>
                                                            <div class="infobox-data" style="margin-top:3px">
                                                                <span class="infobox-data-number ng-binding">1</span>
                                                                <div class="infobox-content" style="text-decoration: underline; color: none;"> @Rental_WebCore2.Resources.Rental_Manager.Rental.Contract_Exp<ul></ul></div>
                                                            </div>

                                                        </div>
                                                    </a>
                                                </div>


                                                @* HD bao thannh ly *@

                                                <div class="btn-outline-primary col-md-6" style="padding-top:3px;border:solid;border-width:1px;border-color:darkgrey">
                                                    <a href="@Url.Action("ContractLiquiddate","RentalManager")">
                                                        <div class="infobox-icon">
                                                            <img src="~/wwwroot/images/Rental/Dashboard/icons8-yellow-file-512.png" style="width:40px;margin-left:0px;margin-top:5px" />
                                                        </div>
                                                        <div class="infobox-data" style="margin-top:3px">
                                                            <span class="infobox-data-number ng-binding">0</span>
                                                            <div class="infobox-content" style="text-decoration: underline; color: none;">@Rental_WebCore2.Resources.Rental_Manager.Rental.Contract_Liquidate<ul></ul></div>
                                                        </div>
                                                    </a>
                                                </div>


                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    @* Tai chinh *@
                    <div class="tile">
                        <h5 class="widget-title">@Rental_WebCore2.Resources.Rental_Manager.Rental.Finance</h5>
                        <hr />
                        <div class="widget-body">
                            <div class="widget-main">
                                <div id="Dashboard_Invoice">

                                    <p><b>@Rental_WebCore2.Resources.Rental_Manager.Rental.Rental_Fee </b></p>
                                    <div class="clearfix">
                                        <div class="col-md-12">
                                            <div class="row">
                                                <a class="btn-outline-primary col-md-6 " onclick="ProductMonthPayment(0)" href="#" asp-area=" " style="border:solid;border-color:darkgray;border-width:1px;">

                                                    <div class="infobox-icon">
                                                        <img src="~/wwwroot/images/Rental/Dashboard/icons8-cheap-$1-50.png" />
                                                    </div>
                                                    <div class="infobox-data">
                                                        <span class="infobox-data-number ng-binding">0</span>
                                                        <div class="infobox-content" style="text-decoration: underline; color: none;">@Rental_WebCore2.Resources.Rental_Manager.Rental.Products_Month_Payment<ul></ul></div>
                                                    </div>
                                                    @*<div class="badge badge-warning">
                                                            0 VNĐ
                                                        </div>*@

                                                </a>


                                                <a class="btn-outline-primary col-md-6" style="border:solid;border-color:darkgray;border-width:1px; " onclick="ProductOverduePayment(0)"href="#">

                                                    <div class="infobox-icon">
                                                        <img src="~/wwwroot/images/Rental/Dashboard/icons8-cheap-$2-50.png" />
                                                    </div>
                                                    <div class="infobox-data">
                                                        <span class="infobox-data-number ng-binding">8</span>
                                                        <div class="infobox-content" style="text-decoration: underline; color: none;">@Rental_WebCore2.Resources.Rental_Manager.Rental.Product_Overdue_Payment<ul></ul></div>
                                                    </div>
                                                    @*<div class="badge badge-danger">
                                                            75.311.676 VNĐ
                                                        </div>*@

                                                </a>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <script>
                    $(function () {
                        $("#Dashboard_Summary").load("/RealEstateReport/Dashboard_Summary");
                        $("#Dashboard_Invoice").load("/RealEstateReport/Dashboard_Invoice");
                        $("#Dashboard_Contract").load("/RealEstateReport/Dashboard_Contract");
                        $("#Dashboard_SummaryInvoiceEW").load("/RealEstateReport/Dashboard_SummaryInvoiceEW");
                    });
                </script>
                <!--PAGE CONTENT ENDS HERE-->
            </div>
        </div>
    </main>
</body>
</html>

<style>
    .btn-outline-primary {
        background-color: white;
        color: #009688;
        font-size: 13px;
        border: solid 1px;
        border-color: darkgray;
        text-align: left;
    }

    .tile-menu {
        display: none;
        background-color: white;
        max-width: 10px;
        margin-left: 38px;
        border: solid 1px;
        border-color: darkgray;
    }

    .dropbtn:hover, .dropbtn:focus {
        background-color: #009688;
    }

    .tile a:hover {
        background-color: #009688;
        color: white;
    }
</style>

@* Sản phẩm đến tháng thu phí *@
<div class="modal fade" id="myModalProductMonthPayment">
    <div class="modal-dialog">
        <div class="modal-content" style="height:auto;width:1300px;margin-left:-410px">
            <text class="form-control" style="background-color:#007065; height:40px;color:aliceblue;border:hidden; text-align:center;font-size:20px; padding-left:0px;border-radius:unset" id="modalTitleProductMonthPayment;">
                @Rental_WebCore2.Resources.Rental_Manager.Rental.Products_Month_Payment (5)
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="color:white; font-size:30px">
                    <span aria-hidden="true">&times;</span>
                </button>
            </text>

            <div class="modal-body">
                <form id="form">
                    <div class="col-md-12">
                        <div class="row">
                            <div class="tile" id="HD">
                                @for (var k = 0; k < 4; k++)
                                {
                                    <div class="row">
                                        @for (var j = 0; j < 3; j++)
                                        {
                                            <div class="col-lg-4">
                                                @{name[i] = "Sản phẩm đến hết hạn thanh toán " ;
                                                    <a class="list-group-item list-group-item-action" href="@Url.Action("DetailContract", "RentalManager")" style="border-color:#009688">
                                                        <img src="~/wwwroot/images/Rental/Contract/icons8-profile-48.png" style="width:30px;height:30px;margin-top:-15px;margin-left:0px" />
                                                        <samp style="font-size:18px">P.123</samp>
                                                        <hr style="background-color:#009688;margin-top:0px" />
                                                        <div style="margin-top:-14px">
                                                            <div class="row">
                                                                <samp>@Rental_WebCore2.Resources.Rental_Manager.Rental.Renter : Chu Chỉ Nhược</samp>

                                                                <samp>@Rental_WebCore2.Resources.Rental_Manager.Rental.Property : Vina/BBT/P.132</samp>
                                                                <samp style="color:red">@Rental_WebCore2.Resources.Rental_Manager.Rental.Rental_And_Service_Fees : 8.000.000</samp>
                                                                <samp style="color:red">@Rental_WebCore2.Resources.Rental_Manager.Rental.Term_Payment : 22/2/2019</samp>

                                                            </div>
                                                        </div>
                                                    </a>

                                                    i = i + 1;
                                                }
                                            </div>

                                        }
                                    </div>
                                    <br />
                                }

                            </div>

                        </div>

                    </div>

                </form>
                <div>
                    @*<a class="btn btn-primary" style="background-color: #007065;color:white">@Rental_WebCore2.Resources.Rental_Manager.Rental.Save</a>
                        <a class="btn btn-primary" style="background-color: #007065;color:white;margin-left:15px" onclick="AddService(0)">+ Thêm dịch vụ</a>*@
                    <a class="btn btn-primary" style="background-color: #007065;color:white;margin-left:15px" data-dismiss="modal">@Rental_WebCore2.Resources.Rental_Manager.Rental.Exit</a>

                </div>
            </div>
        </div>
    </div>
</div>

@* Sản phẩm quá hạn thu phí *@
<div class="modal fade" id="myModalProductOverduePayment">
    <div class="modal-dialog">
        <div class="modal-content" style="height:auto;width:1300px;margin-left:-410px">
            <text class="form-control" style="background-color:#007065; height:40px;color:aliceblue;border:hidden; text-align:center;font-size:20px; padding-left:0px;border-radius:unset" id="modalTitleProductOverduePayment;">
                @Rental_WebCore2.Resources.Rental_Manager.Rental.Product_Overdue_Payment (5)
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="color:white; font-size:30px">
                    <span aria-hidden="true">&times;</span>
                </button>
            </text>

            <div class="modal-body">
                <form id="form">

                    <div class="col-md-12">
                        <div class="row">
                            <div class="tile" id="HD">
                                @for (var k = 0; k < 4; k++)
                                {
                                    <div class="row">
                                        @for (var j = 0; j < 3; j++)
                                        {
                                            <div class="col-lg-4">
                                                @{
                                                    <a class="list-group-item list-group-item-action" href="@Url.Action("DetailContract", "RentalManager")" style="border-color:#009688">
                                                        <img src="~/wwwroot/images/Rental/Contract/icons8-profile-48.png" style="width:30px;height:30px;margin-top:-15px;margin-left:0px" />
                                                        <samp style="font-size:18px">P.123</samp>
                                                        <hr style="background-color:#009688;margin-top:0px" />
                                                        <div style="margin-top:-14px">
                                                            <div class="row">
                                                                <samp>@Rental_WebCore2.Resources.Rental_Manager.Rental.Renter : Trình Giảo Kim</samp> <br>
                                                                <samp>@Rental_WebCore2.Resources.Rental_Manager.Rental.Property : H3/Bình An/P.125</samp><br>
                                                                <samp style="color:red">@Rental_WebCore2.Resources.Rental_Manager.Rental.Rental_And_Service_Fees : 12.000.000</samp><br />
                                                                <samp style="color:red">@Rental_WebCore2.Resources.Rental_Manager.Rental.Term_Payment : 22/2/2019</samp><br />
                                                                <samp style="color:red">@Rental_WebCore2.Resources.Rental_Manager.Rental.Overdue_Payment : 2 ngày</samp><br />

                                                            </div>
                                                        </div>
                                                    </a>

                                                    i = i + 1;
                                                }
                                            </div>

                                        }
                                    </div>
                                    <br />
                                }

                            </div>

                        </div>

                    </div>
                           

                </form>
                <div>
                    @*<a class="btn btn-primary" style="background-color: #007065;color:white">@Rental_WebCore2.Resources.Rental_Manager.Rental.Save</a>
                        <a class="btn btn-primary" style="background-color: #007065;color:white;margin-left:15px" onclick="AddService(0)">+ Thêm dịch vụ</a>*@
                    <a class="btn btn-primary" style="background-color: #007065;color:white;margin-left:15px" data-dismiss="modal">@Rental_WebCore2.Resources.Rental_Manager.Rental.Exit</a>

                </div>
            </div>
        </div>
    </div>
</div>

<script>
    function ProductMonthPayment(ProductMonthPayment) {
        $("#form")[0].reset();
        $("#modalTitleProductMonthPayment").html("Sản phẩm đến hạn thanh toán");
        $("#myModalProductMonthPayment").modal();
    }

    function ProductOverduePayment(ProductOverduePayment) {
        $("#form")[0].reset();
        $("#modalTitleProductOverduePayment").html("Sản phẩm đến hạn thanh toán");
        $("#myModalProductOverduePayment").modal();
    }
</script>


