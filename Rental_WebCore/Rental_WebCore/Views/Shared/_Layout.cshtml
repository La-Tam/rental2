                    <li><a class="treeview-item" asp-area="" asp-controller="RentalManager" asp-action="DashboardIndex"><i class="icon fa fa-circle-o"></i> Dashboard cho thuê</a></li>
                    <li><a class="treeview-item" asp-area="" asp-controller="RentalManager" asp-action="ContractIndex"><i class="icon fa fa-circle-o"></i> Hợp đồng</a></li>
