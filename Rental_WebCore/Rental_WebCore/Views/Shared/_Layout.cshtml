            <li class="treeview">
                <a class="app-menu__item" href="#" data-toggle="treeview"><i class="app-menu__icon fa fa-cog"></i><span class="app-menu__label">Cài đặt</span><i class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" asp-area="" asp-controller="Property" asp-action="Dashboard"><i class="icon fa fa-circle-o"></i> Cài đặt người dùng</a></li>
                    <li><a class="treeview-item" asp-area="" asp-controller="Property" asp-action="Repair"><i class="icon fa fa-circle-o"></i> Cài đặt cho thuê</a></li>
                    <li><a class="treeview-item" asp-area="" asp-controller="Property" asp-action="ToDoList"><i class="icon fa fa-circle-o"></i> Cài đặt khác</a></li>
                </ul>
            </li>
            <li class="treeview">
                <a class="app-menu__item" href="#" data-toggle="treeview"><i class="app-menu__icon fa fa-leanpub"></i><span class="app-menu__label">Hướng dẫn sử dụng</span><i class="treeview-indicator fa fa-angle-right"></i></a>
